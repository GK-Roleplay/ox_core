{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "types": ["types/index.ts"],
      "config": ["server/config.ts"],
      "db": ["server/db/index.ts"],
      "accounts": ["server/accounts/index.ts"],
      "accounts/*": ["server/accounts/*"],
      "groups": ["server/groups/index.ts"],
      "groups/*": ["server/groups/*"],
      "player/*": ["server/player/*"],
      "vehicle": ["server/vehicle/index.ts"],
      "vehicle/*": ["server/vehicle/*"],
      "bridge/*": ["server/bridge/*"]
    },
    "skipLibCheck": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "es2020",
    "noEmit": true
  },
  "include": ["client", "server", "common", "types"]
}
  